# Given a 500 return code, expect change to 404
test {
  name: "With500StatusCodeChangeTo404"
  response_headers {
    input { header { key: ":status" value: "500" } }
    result {
      headers {
        has_header { key: ":status" value: "404" }
      }
    }
  }
}
