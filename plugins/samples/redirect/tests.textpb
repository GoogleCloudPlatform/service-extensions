# No expected URL, no redirect.
test {
  name: "NoRedirect"
  benchmark: false
  request_headers {
    input { header { key: "Location" value: "www.example.com" } } 
    result { has_header { key: "Location" value: "www.example.com" } }
  }
}
# Wirh expected URL, expected imediate with http status code 301 and
# redirect to other location.
test {
  name: "DoRedirect"
  benchmark: false
  request_headers {
    input { header { key: "Location" value: "https://service-extensions.com/main/somepage" } } 
    result { 
      immediate { http_status: 301 details: "" }
      has_header { key: "Location" value: "https://service-extensions.com/redirect" }
    }
  }
}